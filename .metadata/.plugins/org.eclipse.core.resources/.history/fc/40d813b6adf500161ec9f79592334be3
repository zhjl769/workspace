#ifndef VARIABLE_INIT_H_
#define VARIABLE_INIT_H_

//变量定义

int16 Piont1, Piont2;
Uint16 FLAG = 0;                    //此标志是一个什么标志？定时器中断服务程序中有对FLAG置1的操作，应该是开始采集的标志
unsigned int    data_cnt=0;
unsigned int     ad_signal_num=0;

//参考信号部分变量定义
#define PI        3.14159265
#define fs            18  //AD的采样率,单位：kHz
#define Time_Ref      20  //参考信号脉宽,单位：ms
