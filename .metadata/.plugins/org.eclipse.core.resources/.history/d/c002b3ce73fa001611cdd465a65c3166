		.sect "vectors"	        ;section for vectors
		.ref  _boot
		.ref _c_int4
		.ref _c_int5
		.ref _c_int6
		.ref _c_int7
		.ref _c_int8
		.ref _c_int9
		.ref _c_int10		
		.ref _c_int00	        ;entry address
RESET_RST:
;		mvkl .S2 _c_int00,B0
;		mvkh .S2 _c_int00,B0
		mvkl .S2 _boot,B0
		mvkh .S2 _boot,B0
		B    .S2 B0
		.loop 5     			
		NOP						
	    .endloop 
	    
NMI_RST: ;b _c_int1
		.loop 8
		 NOP    			
		 .endloop 
RESV1:	;b _c_int2
		.loop 8
		NOP 				
		.endloop
RESV2:	;b _c_int3
		.loop 8
		NOP				
		.endloop

INT4:   b   _c_int4
		.loop 7
		NOP  				
		.endloop	
INT5:	b   _c_int5
	 	.loop 7
		NOP
		.endloop
INT6:  	b   _c_int6
	 	.loop 7
		NOP
		.endloop
INT7:	b   _c_int7
	 	.loop 7
	 	NOP
		.endloop
INT8: 	b   _c_int8
		.loop 7
		NOP
		.endloop
INT9:	b   _c_int9
		.loop 7
		NOP
		.endloop
INT10:  b  _c_int10					
	    .loop 7
		NOP
		.endloop

INT11: 	;b 	_c_int11		
		.loop 8	
		NOP
		.endloop

INT12: 	;b _c_int12
		.loop 8
		NOP
		.endloop
INT13:	;b _c_int13
		.loop 8
		NOP
		.endloop
INT14: 	;b _c_int14
