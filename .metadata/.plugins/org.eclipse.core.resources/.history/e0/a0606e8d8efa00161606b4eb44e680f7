*---------vecs.asm---------
*
* Assembly file to set up interrupt service table (IST)
*
*------------------------------------------------------------------------------
* Global symbols defined here and exported out of this file
*------------------------------------------------------------------------------
   .sect ".vectors"     ;section for vectors
   .ref _c_int00
   .ref EDMA0_int       ; EDMA0_int interrupt handler
   .ref Mcbsp0_RX_int   ; Mcbsp0_RX_int interrupt handler

*------------------------------------------------------------------------------
* Global symbols defined here and exported out of this file
*------------------------------------------------------------------------------

   .global _VECSTART

*------------------------------------------------------------------------------
* This is the actual interrupt service table (IST). It is properly aligned and
* is located in the subsection .text:vecs. This means if you don't explicitly
* specify this section in your linker command file, it will default and link
* into the .text section. Remember to set the ISTP register to point to this
* table.
*------------------------------------------------------------------------------
RESET_RST:
    mvkl .S2 _c_int00, B0
    mvkh .S2 _c_int00, B0
    B    .S2 B0
         .loop 5
		 NOP
	    .endloop

NMI_RST:
      b  nrp
      .loop 5
	   NOP
	  .endloop

RESV1:
      b  irp
     .loop 5
	NOP
	 .endloop

RESV2:
      b  irp
      .loop 5
		NOP
	  .endloop

INT4:
      b  irp
     .loop 5
		NOP
	 .endloop

INT5:
      b  irp
      .loop 5
		NOP
	  .endloop

INT6:
      b  irp
      .loop 5
		NOP
	  .endloop

INT7:
      b  irp
      .loop 5
 		NOP
	  .endloop

INT8:
      b  EDMA0_int
     .loop 5
		NOP
	 .endloop

INT9:
      b  irp
     .loop 5
		NOP
	 .endloop

INT10:
      b  irp
      .loop 5
		NOP
	  .endloop

INT11:
      b  irp
      .loop 5
		NOP
	  .endloop

INT12:
      b  irp
      .loop 5
		NOP
	  .endloop

INT13:
      b  Mcbsp0_RX_int
      .loop 5
		NOP
	  .endloop

INT14:
      b  irp
      .loop 5
		NOP
	  .endloop

INT15:
      b  irp
     .loop 5
	 NOP
	 .endloop
