/****************************************************
º¯Êı£ºvoid MMC_Init(void)
¹¦ÄÜ£º³õÊ¼»¯MMC¿ØÖÆÆ÷
ÊäÈë²ÎÊı£ºÎŞ
·µ»Ø£ºÎŞ
****************************************************/
void MMC_Init(void)
{
  short SD_jishu;
  //×¢£º¶ÔÓÚ5509aµÄMMC¿ØÖÆÆ÷£¬Ö»Ö§³ÖMMC¿¨ºÍSD¿¨
  mmc1 = MMC_open(MMC_DEV1);//Ê¹ÄÜMMC0¿ØÖÆÆ÷
  MMC_setupNative(mmc1,&Init);//°´ÕÕ³õÊ¼²ÎÊıÀ´³õÊ¼»¯MMC0¿ØÖÆÆ÷

  MMC_sendGoIdle(mmc1);//¸´Î»×ÜÏßÉÏµÄËµÓĞ´æ´¢¿¨
  for (count=0;count<4016;count++)
  {
  	asm(" NOP ");
  }
  SD_jishu = 0;
  do
  {
	  cardtype = MMC_sendOpCond(mmc1,0x00100000);//¹¤×÷µçÑ¹ÃÅÏŞ£º3.2~3.3V£¬²»Í¬µÄ¿¨¶Ô¸ÃÃüÁîµÄÓ¦´ğÖµ²»Í¬£¬MMC¿ØÖÆÆ÷¸ù¾İÓ¦´ğÖµÀ´ÅĞ¶Ï¸Ã´æ´¢¿¨ÀàĞÍ
	  SD_jishu++;
	  if(SD_jishu == 10)
	  {
		SD_status = 0;
		break;
	  }
  }while(cardtype == 65535);
  
  if(cardtype == 0xffff)
  {
	SD_status = 0;
    
  }
  else
  {
     //¼ì²âµ½ÎªSD¿¨
     cid = &cardid;
     SD_sendAllCID(mmc1,cid); // get the CID structure for all cards.     
     card = &cardalloc;
