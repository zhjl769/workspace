void get_voltage()
{
	long sum;
	short i = 0;
	for ( i = 0; i < 64; i++)
	{
		ADCCTL = 0x8000;	// Æô¶¯AD×ª»»£¬Í¨µÀ0
		do
		{
		    temp_ADC=ADCDATA;
		}while(temp_ADC & 0x8000);//²éÑ¯ADCÃ¦±êÖ¾Î»
		ADC_in[i] = temp_ADC & 0x0fff;
	}
	sum = 0;
	for(i = 0; i < 64; i++)
	{
		sum = sum + ADC_in[i];
	}
	voltage_AD = (short)(sum>>6);//ÇóµçÑ¹¾ùÖµ
}


void get_parameter()//»ñÈ¡²ÎÊıĞÅÏ¢
